{
	"manifest_version": 2,
	"name": "vipvideo",
	"version": "3.0",
	"description": "vipvideo,coded by LMoon",
	"browser_action": {
		"default_title": "vipvideo",
		"default_popup": "vipvideo.html"
	},
	"content_security_policy": "script-src 'self'; object-src 'self'",
	"content_scripts": [{
		"matches": [
			"*://*/*"
		],
		"js": [
			"do.js"
		]
	}],
	"permissions": ["*://*/*","tabs"]
}
