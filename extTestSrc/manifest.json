{
	"manifest_version": 2,
	"name": "bdcCrack",
	"version": "1.0",
	"description": "bdcCrack",
	"browser_action": {
		"default_title": "bdcCrack",
		"default_popup": "main.html"
	},
	"content_security_policy": "script-src 'self'; object-src 'self'",
	"content_scripts": [
		{
			"matches": [
				"*://pan.baidu.com/*"
			],
			"js": [
				"do.js",
				"jquery-3.2.0.min.js"
			]
		}
	],
	"permissions": [
		"*://*/*",
		"tabs",
		"cookies", 
		"webRequest", 
		"webRequestBlocking"
	],
	"background": {
	  "scripts": [
		"main.js"
	  ]
	}
}
