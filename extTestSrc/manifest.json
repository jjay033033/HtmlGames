{
	"manifest_version": 2,
	"name": "test",
	"version": "1.0",
	"description": "test",
	"browser_action": {
		"default_title": "test",
		"default_popup": "vipvideo.html"
	},
	"content_security_policy": "script-src 'self'; object-src 'self'",
	"content_scripts": [
		{
			"matches": [
				"*://pan.baidu.com/*"
			],
			"js": [
				"do.js",
				"jquery-3.2.0.min.js"
			]
		}
	],
	"permissions": [
		"*://*/*",
		"tabs",
		"cookies", 
		"webRequest", 
		"webRequestBlocking"
	]
}
